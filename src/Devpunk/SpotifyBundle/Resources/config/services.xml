<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="devpunk.spotify.session" class="SpotifyWebAPI\Session">
            <argument>%devpunk.spotify.client_id%</argument>
            <argument>%devpunk.spotify.client_secret%</argument>
            <argument>%devpunk.spotify.client_redirect_uri%</argument>
        </service>
    </services>
</container>
